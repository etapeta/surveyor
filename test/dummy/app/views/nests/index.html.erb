<h1>Listing nests</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Document</th>
    <th>Header</th>
    <th>Footer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @nests.each do |nest| %>
  <tr>
    <td><%= nest.name %></td>
    <td><%= nest.document %></td>
    <td><%= nest.header %></td>
    <td><%= nest.footer %></td>
    <td><%= link_to 'Show', nest %></td>
    <td><%= link_to 'Edit', edit_nest_path(nest) %></td>
    <td><%= link_to 'Destroy', nest, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Nest', new_nest_path %>
